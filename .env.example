# Google Custom Search API Configuration
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_SEARCH_ENGINE_ID=your_custom_search_engine_id_here

# MCP Server Configuration
LOG_LEVEL=info

# Redmine Configuration (Multi-Repository Support)
# These API keys will be referenced from redmine-repositories.json using ${VARIABLE_NAME} syntax
REDMINE_MAIN_API_KEY=your_main_redmine_api_key_here
REDMINE_DEV_API_KEY=your_dev_redmine_api_key_here
REDMINE_STAGING_API_KEY=your_staging_redmine_api_key_here

# Legacy Redmine Configuration (Single Repository - backward compatibility)
# These are used if redmine-repositories.json is not found
REDMINE_URL=https://redmine.example.com
REDMINE_API_KEY=your_legacy_api_key_here

# Optional: Custom path to Redmine repositories configuration file
# REDMINE_CONFIG_PATH=./custom-redmine-config.json
